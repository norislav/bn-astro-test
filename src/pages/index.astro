---
import MainLayout from "../layouts/MainLayout.astro"
import Hero from '../components/HeroHome.astro';
import About from "../components/About.astro";

import HomeData from '../content/pages/home.json';
import GeneralData from '../content/general/general.json';
import ProgramCards from "../components/ProgramCards.astro";
import SingleTestimonial from "../components/SingleTestimonial.astro";
import StickyContent from "../components/StickyContent.astro";

---

<MainLayout title="Home">
	<Hero HeroData={HomeData.hero} GeneralData={GeneralData}/>
	<About AboutData={HomeData.about}/>
	<ProgramCards />
	<SingleTestimonial />
	<StickyContent />

</MainLayout>